<scxml 
  initial="idle" 
  version="1.0" 
  name="AlarmBatteryLowSM"
  xmlns="http://www.w3.org/2005/07/scxml">

    <state id="idle">
      <transition event="CMD_START" target="alarm" />
    </state>

    <state id="ready">
      <transition event="CMD_ALARM" target="alarm" />
    </state>


    <state id="alarm">
    </state>
</scxml>

