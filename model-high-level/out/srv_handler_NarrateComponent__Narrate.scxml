<scxml name="srv_handler_NarrateComponent__Narrate" version="1.0" model_src="" initial="waiting" xmlns="http://www.w3.org/2005/07/scxml">
    <datamodel>
        <!-- TYPE ros_fields__command:string -->
        <data id="ros_fields__command" expr="''"/>
        <!-- TYPE ros_fields__is_ok:bool -->
        <data id="ros_fields__is_ok" expr="false"/>
        <!-- TYPE ros_fields__error_msg:string -->
        <data id="ros_fields__error_msg" expr="''"/>
    </datamodel>
    <state id="waiting">
        <transition event="srv_NarrateComponent__Narrate_req_client_NarratePoiSkill" target="processing_client_NarratePoiSkill">
            <assign location="ros_fields__command" expr="_event.data.ros_fields__command"/>
            <send event="srv_NarrateComponent__Narrate_request" target="NarrateComponent">
                <param name="ros_fields__command" expr="ros_fields__command"/>
            </send>
        </transition>
    </state>
    <state id="processing_client_NarratePoiSkill">
        <transition event="srv_NarrateComponent__Narrate_response" target="waiting">
            <assign location="ros_fields__is_ok" expr="_event.data.ros_fields__is_ok"/>
            <assign location="ros_fields__error_msg" expr="_event.data.ros_fields__error_msg"/>
            <send event="srv_NarrateComponent__Narrate_response_client_NarratePoiSkill" target="NarratePoiSkill">
                <param name="ros_fields__is_ok" expr="ros_fields__is_ok"/>
                <param name="ros_fields__error_msg" expr="ros_fields__error_msg"/>
            </send>
        </transition>
    </state>
</scxml>