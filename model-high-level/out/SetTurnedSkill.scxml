<scxml name="SetTurnedSkill" version="1.0" model_src="" initial="idle" xmlns="http://www.w3.org/2005/07/scxml">
    <datamodel>
        <!-- TYPE FIELD_NAME:string -->
        <data id="FIELD_NAME" expr="'turnBackState'"/>
        <!-- TYPE VALUE_STR:string -->
        <data id="VALUE_STR" expr="'turned'"/>
        <!-- TYPE result:bool -->
        <data id="result" expr="false"/>
        <!-- TYPE SKILL_SUCCESS:int8 -->
        <data id="SKILL_SUCCESS" expr="0"/>
        <!-- TYPE SKILL_FAILURE:int8 -->
        <data id="SKILL_FAILURE" expr="1"/>
        <!-- TYPE SKILL_RUNNING:int8 -->
        <data id="SKILL_RUNNING" expr="2"/>
    </datamodel>
    <state id="idle">
        <transition event="srv_SetTurnedSkill__tick_request" target="setString">
            <send event="srv_BlackboardComponent__SetString_req_client_SetTurnedSkill" target="srv_handler_BlackboardComponent__SetString">
                <param name="ros_fields__field_name" expr="FIELD_NAME"/>
                <param name="ros_fields__value" expr="VALUE_STR"/>
            </send>
        </transition>
        <transition event="srv_SetTurnedSkill__halt_request" target="halted"/>
    </state>
    <state id="setString">
        <transition event="srv_BlackboardComponent__SetString_response_client_SetTurnedSkill" target="afterSet">
            <assign location="result" expr="_event.data.ros_fields__is_ok"/>
        </transition>
        <transition event="srv_SetTurnedSkill__halt_request" target="halted"/>
    </state>
    <state id="afterSet">
        <transition cond="result" target="waitInc">
            <send event="srv_TurnBackManagerComponent__IncreaseTurnBacksCounter_req_client_SetTurnedSkill" target="srv_handler_TurnBackManagerComponent__IncreaseTurnBacksCounter"/>
        </transition>
        <transition target="idle"/>
    </state>
    <state id="waitInc">
        <transition event="srv_TurnBackManagerComponent__IncreaseTurnBacksCounter_response_client_SetTurnedSkill" target="reply">
            <assign location="result" expr="_event.data.ros_fields__is_ok"/>
        </transition>
        <transition event="srv_SetTurnedSkill__halt_request" target="halted"/>
    </state>
    <state id="reply">
        <onentry>
            <if cond="result">
                <send event="srv_SetTurnedSkill__tick_response" target="srv_handler_SetTurnedSkill__tick">
                    <param name="ros_fields__status" expr="SKILL_SUCCESS"/>
                    <param name="ros_fields__is_ok" expr="true"/>
                </send>
                <else/>
                <send event="srv_SetTurnedSkill__tick_response" target="srv_handler_SetTurnedSkill__tick">
                    <param name="ros_fields__status" expr="SKILL_FAILURE"/>
                    <param name="ros_fields__is_ok" expr="true"/>
                </send>
            </if>
        </onentry>
        <transition target="idle"/>
    </state>
    <state id="halted">
        <onentry>
            <assign location="result" expr="false"/>
            <send event="srv_SetTurnedSkill__halt_response" target="srv_handler_SetTurnedSkill__halt">
                <param name="ros_fields__is_ok" expr="true"/>
            </send>
        </onentry>
        <transition target="idle"/>
    </state>
</scxml>