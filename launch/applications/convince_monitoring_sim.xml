<application>
    <name>convince_monitoring_sim</name>

    <module>
        <name>./clean_monitors.sh</name>
		<workdir>/home/user1/UC3/monitoring</workdir>
        <node>monitoring</node>
    </module>

    <module>
        <name>./generate_all.sh</name>
		<workdir>/home/user1/UC3/monitoring</workdir>
        <node>monitoring</node>
    </module>

    <module>
        <name>./launch_other.sh</name>
		<workdir>/home/user1/UC3/monitoring</workdir>
        <node>monitoring</node>
    </module>

    <module>
        <name>./launch_prop10POI.sh</name>
		<workdir>/home/user1/UC3/monitoring</workdir>
        <node>monitoring</node>
    </module>

    <module>
        <name>./stop_other.sh</name>
		<workdir>/home/user1/UC3/monitoring</workdir>
        <node>monitoring</node>
    </module>

    <module>
        <name>./stop_prop10POI.sh</name>
		<workdir>/home/user1/UC3/monitoring</workdir>
        <node>monitoring</node>
    </module>

    <module>
        <name>python3 multi_monitor.py</name>
		<workdir>/home/user1/UC3/monitoring</workdir>
        <node>monitoring</node>
    </module>

    <module>
        <name>ros2</name>
		 <parameters>run monitoring_clock_component monitoring_clock_component</parameters>
        <node>monitoring</node>
    </module>

    <module>
        <name>ros2</name>
		 <parameters>run network_management_from_monitor network_management_from_monitor</parameters>
        <node>monitoring</node>
    </module>

</application>
